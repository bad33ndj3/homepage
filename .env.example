# Copy this file to `.env` and fill in your own tokens.
# Vite exposes any variable prefixed with VITE_ to the client runtime.

# GitLab personal access token with API scope
VITE_GITLAB_TOKEN=glpat-your-token-here
# Optional: override the GitLab API endpoint that lists merge requests assigned to you
VITE_GITLAB_API_URL=https://gitlab.com/api/v4/merge_requests?scope=assigned_to_me&state=opened
# Optional: GitLab namespace/project for building filtered URLs (e.g., my-group/my-project)
VITE_GITLAB_NAMESPACE=energyzero/backend
